{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome ",
      "type": "chrome",
      "request": "launch",
      "port": 9236,
      "webRoot": "${workspaceFolder}",
      "url": "https:"
    },
    {
      "name": "Launch index.php",
      "type": "php",
      "request": "launch",
      "port": 9000,
      "cwd": "${workspaceFolder}/WebContent",
      "program": "${workspaceFolder}/WebContent/index.php"
    },
    {
      "name": "Launch Chrome localhost",
      "type": "chrome",
      "request": "launch",
      "port": 9203,
      "webRoot": "${workspaceFolder}",
      "url": "http://localhost:8080/RabbitGameDB/index.php"
    }
  ]
}